                                            <yAxis>
                                              <NumberAxis fx:id="stepChartYAxis" side="LEFT" />
                                            </yAxis>
                                          </BarChart>
                                       </children>
                                    </StackPane>
                                 </children>
                              </VBox>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
