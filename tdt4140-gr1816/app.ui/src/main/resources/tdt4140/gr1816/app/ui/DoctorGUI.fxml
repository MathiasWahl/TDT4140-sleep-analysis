                                              <CategoryAxis fx:id="sleepChartXAxis" side="BOTTOM" />
                                            </xAxis>
                                            <yAxis>
                                              <NumberAxis fx:id="sleepChartYAxis" side="LEFT" />
                                            </yAxis>
                                          </BarChart>
                                          <PieChart fx:id="sleepPieChart" disable="true" legendVisible="false" prefHeight="200.0" prefWidth="392.0" title="Sleep Pie Chart" />
